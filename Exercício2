package lpa2;

import java.util.Scanner;

public class Bale {

	public static void main(String[] args) {
	   
		int a [] = new int[5], soma =0;
		Scanner ler = new Scanner(System.in);
		
		for (int i =0; i<5; i++) {
			System.out.println("Informe um valor " + i+ "..:");
			a[i]= ler.nextInt();
			soma = soma+a[i];
		}
        for (int i =0; i<5; i++) {
        	  System.out.println(a[i]);
        }
        if (soma > 15) {
        System.out.println(soma);
        }
	}

}
